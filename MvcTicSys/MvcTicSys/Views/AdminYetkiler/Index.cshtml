@model IEnumerable<MvcTicSys.Tbl_Yetkiler>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
 
<p>
    @Html.ActionLink("Yeni bir yetki olusturmak icin tiklayiniz.", "Create")
</p>
<table>
    <tr>
        <th>
            Yetki
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Yetki)
        </td>
        <td>
            @Html.ActionLink("Duzenle", "Edit", new { id=item.YetkiKodu }) |
            @Html.ActionLink("Detay", "Details", new { id=item.YetkiKodu }) |
            @Html.ActionLink("Sil", "Delete", new { id=item.YetkiKodu })
        </td>
    </tr>
}

</table>
